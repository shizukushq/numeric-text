{
  "name": "@numeric-text/svelte",
  "type": "module",
  "version": "0.1.3",
  "description": "Svelte wrapper for @numeric-text/core",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shizukushq/numeric-text.git"
  },
  "bugs": {
    "url": "https://github.com/shizukushq/numeric-text/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "svelte-package -w",
    "package": "svelte-package",
    "package:watch": "svelte-package --watch"
  },
  "peerDependencies": {
    "svelte": "^5"
  },
  "dependencies": {
    "@numeric-text/core": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "swiftui",
    "NumericText",
    "numeric-text",
    "numericText",
    "animation",
    "animated-text",
    "animated-number",
    "text"
  ]
}
